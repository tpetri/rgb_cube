	default "stm3210e-eval"      if ARCH_BOARD_STM3210E_EVAL
	default "stm3220g-eval"      if ARCH_BOARD_STM3220G_EVAL
	default "stm3240g-eval"      if ARCH_BOARD_STM3240G_EVAL
